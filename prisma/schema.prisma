datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  //directUrl = env("DATABASE_URL_UNPOOLED") // Only for Prisma <5.10
}

generator client {
  provider = "prisma-client-js"
}

model UserDetails {
  id          Int      @id @default(autoincrement())
  username    String   @unique
  password    String
  name        String
  email       String   @unique
  phoneNumber String
  
}
model Product {
  id          Int      @id @default(autoincrement())
  imageUrl    String
  title       String
  description String
  price       Float
  category    String
  rating      Float     
  
}
